{
    "name": "septic-config",
    "displayName": "Septic Language Basics",
    "description": "Provides snippets, syntax highlighting and multi-level folding in Septic config files.",
    "publisher": "Equinor",
    "author": "Einar S. Ids√∏",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/equinor/vscode-septic"
    },
    "version": "0.3.0",
    "icon": "images/SEPTIC_logo.jfif",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:septic"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "septic",
                "aliases": [
                    "Septic"
                ],
                "extensions": [
                    ".cnfg"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "jinja",
                "aliases": [
                    "Jinja Raw",
                    "jinja-raw"
                ],
                "extensions": [
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "septic",
                "scopeName": "source.septic",
                "path": "./syntaxes/septic.tmLanguage.json"
            },
            {
                "language": "jinja",
                "scopeName": "source.jinja",
                "path": "./syntaxes/jinja.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "septic",
                "path": "snippets/snippets.json"
            }
        ],
        "configurationDefaults": {
            "[septic]": {
                "files.encoding": "windows1252",
                "editor.wordWrap": "on",
                "editor.insertSpaces": true
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^5.2.7",
        "@types/vscode": "^1.39.0",
        "mocha": "^6.2.2",
        "typescript": "^3.6.4",
        "vscode-test": "^1.2.0"
    },
    "dependencies": {}
}
