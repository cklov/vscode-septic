{
	"compilerOptions": {
		"outDir": "./out",
		"target": "es2020",
		"lib": [
			"ES2020",
		],
		"module": "commonjs",
		"strict": true,
		"exactOptionalPropertyTypes": false,
		"useUnknownInCatchVariables": false,
		"alwaysStrict": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitOverride": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"src/**/*",
	]
}