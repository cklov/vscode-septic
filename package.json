{
    "name": "septic-config",
    "displayName": "Septic Language Basics",
    "description": "Provides syntax highlighting and snippets in Septic config files.",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.39.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "septic",
            "aliases": ["Septic"],
            "extensions": [".cnfg"],
            "configuration": "./language-configuration.json"
        }],
        "snippets": [{
            "language": "septic",
            "path": "snippets/snippets.json"
        }],
        "grammars": [{
            "language": "septic",
            "scopeName": "source.septic",
            "path": "./syntaxes/septic.tmLanguage.json"
        }],
        "configurationDefaults": {
            "[septic]": {
                "files.encoding": "windows1252"
            }
        }
    }
}