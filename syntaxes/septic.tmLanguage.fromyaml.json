{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"information_for_contributors": [
		"This file has been converted from https://github.com/textmate/yaml.tmbundle/blob/master/Syntaxes/YAML.tmLanguage",
		"If you want to provide a fix or improvement, please create a pull request against the original repository.",
		"Once accepted there, we are happy to receive an update request."
	],
	"version": "https://github.com/textmate/yaml.tmbundle/commit/e54ceae3b719506dba7e481a77cea4a8b576ae46",
	"name": "SEPTIC",
	"scopeName": "source.septic",
	"patterns": [
		{
			"include": "#node"
		}
	],
	"repository": {
		"block-collection": {
			"patterns": [
				{
					"include": "#block-mapping"
				}
			]
		},
		"block-mapping": {
			"patterns": [
				{
					"include": "#block-pair"
				}
			]
		},
		"block-node": {
			"patterns": [
				{
					"include": "#block-scalar"
				},
				{
					"include": "#block-collection"
				},
				{
					"include": "#flow-scalar"
				}
			]
		},
		"block-pair": {
			"patterns": [
				{
					"begin": "(?x)(?=(?x:[^\\s[-?:,\\[\\]{}#&*!|>'\"%@`]] | [?:-] \\S)([^\\s:] |  \\s+ (?![#\\s])\n                            )*\n                            \\s*\n                            :\n\t\t\t\t\t\t\t(\\s|$)\n                        )\n                    ",
					"end": "(?x)\n                        (?=\n                              \\s* $\n                            | \\s+ \\#\n                            | \\s* : (\\s|$)\n                        )\n                    ",
					"patterns": [
						{
							"begin": "(?x)\n                                  [^\\s[-?:,\\[\\]{}#&*!|>'\"%@`]]\n                                | [?:-] \\S\n                            ",
							"beginCaptures": {
								"0": {
									"name": "entity.name.tag.septic"
								}
							},
							"contentName": "entity.name.class.septic",
							"end": "(?x)\n                                (?=\n                                      \\s* $\n                                    | \\s+ \\#\n                                    | \\s* : (\\s|$)\n                                )\n                            ",
							"name": "string.unquoted.plain.out.septic"
						}
					]
				},
				{
					"match": ":(?=\\s|$)",
					"name": "punctuation.separator.key-value.mapping.septic"
				}
			]
		},
		"flow-scalar": {
			"patterns": [
				{
					"include": "#flow-scalar-double-quoted"
				},
				{
					"include": "#flow-scalar-plain-in"
				}
			]
		},
		"flow-scalar-double-quoted": {
			"begin": "\"",
			"beginCaptures": {
				"0": {
					"name": "punctuation.definition.string.begin.septic"
				}
			},
			"end": "\"",
			"endCaptures": {
				"0": {
					"name": "punctuation.definition.string.end.septic"
				}
			},
			"name": "string.quoted.double.septic"
		},
		"flow-scalar-plain-in": {
			"patterns": [
				{
					"begin": "(?x)\n                          [^\\s[-?:,\\[\\]{}#&*!|>'\"%@`]]\n                        | [?:-] [^\\s[\\[\\]{},]]\n                    ",
					"end": "(?x)\n                        (?=\n                              \\s* $\n                            | \\s+ \\#\n                            | \\s* : (\\s|$)\n                            | \\s* : [\\[\\]{},]\n                            | \\s* [\\[\\]{},]\n                        )\n                    ",
				    "name": "string.unquoted.plain.in.septic"
				}
			]
		},
		"node": {
			"patterns": [
				{
					"include": "#block-node"
				}
			]
		}
	}
}